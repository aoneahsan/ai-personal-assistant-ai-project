{
  "name": "ai-personal-assistant",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "set TZ=Europe/Dublin & node server.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint",
    "cap:sync": "yarn install && npm run build && cap sync",
    "cap:sync:only": "cap sync",
    "cap:android:add": "cap add android",
    "cap:android:update": "cap update android",
    "cap:android:run": "cap run android",
    "cap:android:open": "cap open android",
    "gen:android:assets": "capacitor-assets generate --android",
    "sync:apps-config": "trapeze run apps-config.yaml",
    "cap:ios:add": "cap add ios",
    "cap:ios:update": "cap update ios",
    "cap:ios:run": "cap run ios",
    "cap:ios:open": "cap open ios",
    "gen:ios:assets": "capacitor-assets generate --ios",
    "update:ios:info": "tsx scripts/ios-app-config.ts",
    "update:android:info": "tsx scripts/android-app-config.ts",
    "prettier": "prettier --write .",
    "gen:assets": "yarn gen:android:assets && yarn gen:ios:assets",
    "deploy:web": "npm run build && firebase deploy --only hosting",
    "deploy:staging": "npm run build:staging && firebase deploy --only hosting:staging",
    "deploy:production": "npm run build:production && firebase deploy --only hosting:production",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "gen:web:assets": "capacitor-assets generate --web"
  },
